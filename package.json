{
  "name": "atproto-auth-vue-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "npm run build && wrangler dev",
    "init-dpop-session-store": "wrangler kv namespace create DPOP_SESSION_STORE",
    "init-dpop-state-store": "wrangler kv namespace create DPOP_STATE_STORE",
    "build-only": "vite build",
    "gen-lexicons-types": "lex gen-server ./src/lexicon ./lexicons/*",
    "type-check": "vue-tsc --build",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@atproto-labs/handle-resolver": "^0.3.0",
    "@atproto-labs/simple-store": "^0.2.0",
    "@atproto/api": "^0.15.23",
    "@atproto/common-web": "^0.4.2",
    "@atproto/jwk": "^0.4.0",
    "@atproto/jwk-webcrypto": "^0.1.9",
    "@atproto/lexicon": "^0.4.11",
    "@atproto/oauth-client": "^0.5.0",
    "@atproto/xrpc-server": "^0.8.0",
    "cookie": "^1.0.2",
    "iron-webcrypto": "^1.2.1",
    "itty-router": "^5.0.18",
    "multiformats": "^13.3.7",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@atproto/jwk-jose": "^0.1.9",
    "@atproto/lex-cli": "^0.8.3",
    "@cloudflare/vite-plugin": "^1.9.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "daisyui": "^5.0.43",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.23.0"
  }
}
